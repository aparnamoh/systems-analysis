#include <iostream>
#include <iomanip>
#include <cmath>

using namespace std;

int main(void){
	int n, i;
	cout<<"Enter a number to be factorized into its primes: "<<endl;
	cin>>n;
	
	while(n%2==0){
		cout<<"2 ";
		n = n/2;
	}
	
	for (i=3; i<=n; i++){
		while(n%i==0){
			cout<<i<<" ";
			n = n/i;
		}
		
	}
	return 0;	
}
